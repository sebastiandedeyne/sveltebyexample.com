		"cheerio": "^1.0.0-rc.12",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^8.2.1",
		"marked": "^7.0.4",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"shiki": "^0.14.4",
		"svelte": "^4.0.5",
		"vite": "^4.4.2"
	},
	"type": "module"
